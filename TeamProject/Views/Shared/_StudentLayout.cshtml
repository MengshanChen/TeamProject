     <!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Dashboard", "Dashboard", "Student")</li>
                    <li>@Html.ActionLink("Profile", "Profile", "Student")</li>
                    <li>@Html.ActionLink("WorldTour", "GameInfo", "Student")</li>
                    <li>@Html.ActionLink("Record", "Record", "Student")</li>
                    <li>@Html.ActionLink("Calendar", "Calendar", "Student")</li>
                </ul>
                <ul class="nav navbar-nav navbar-right" style="margin-right:30px;">
                    <li>@Html.ActionLink("Log Out", "Index", "Home")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
    </div>
    <div class="container body-content">
        <hr />
        <center>
            <footer>
                <div class="footer">
                    <div style="background-color:transparent">
                        &nbsp;&nbsp;Copyrights &copy; @DateTime.Now.Year U-High Team All rights reserved
                        &nbsp;&nbsp;
                        @Html.ActionLink("About", "About", "Home", new { controller = "Home" }, new { @class = "btn btn-link" })
                        @Html.ActionLink("Privacy Policy", "Privacy", "Home", new { controller = "Home" }, new { @class = "btn btn-link" })
                        @Html.ActionLink("Contact", "Contact", "Home", new { controller = "Home" }, new { @class = "btn btn-link" })
                    </div>
                </div>
            </footer>
        </center>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

<style>
    .navbar-inverse {
        background-color: #083e52;
        border-color: #083e52;
    }

        .navbar-inverse .navbar-nav > li > a {
            color: white;
        }

    .btn-link {
        color: #083e52;
    }

    body {
        color: #083e52;
    }
</style>

